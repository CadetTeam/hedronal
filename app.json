{
  "expo": {
    "name": "Hedronal",
    "slug": "Hedronal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5DC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.responsenow.Hedronal",
      "infoPlist": {
        "NSContactsUsageDescription": "This app needs access to your contacts to invite people to join your entities."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/dark.png",
        "backgroundColor": "#F5F5DC"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.responsenow.Hedronal",
      "permissions": [
        "READ_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/dark.png"
    }
  }
}
